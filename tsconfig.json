{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": [
      "dom",
      "es6"
    ],
    "sourceMap": true,
    "outDir": "./dist/",
    "removeComments": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "page": [
        "*",
        "page/*",
      ],
      "http": [
        "Http/*"
      ],
      "assing": [
        "assing/*"
      ]
    },
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}